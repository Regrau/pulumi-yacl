{
  "name": "TFProvider",
  "build": {
      "dockerfile": "Dockerfile"
  },
  "features": {
    "ghcr.io/devcontainers/features/common-utils": {
      "username": "vscode",
      "configureZshAsDefaultShell": true
    }
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "golang.go",
        "GitHub.vscode-github-actions"
      ]
    }
  },
  "containerUser": "vscode"
}